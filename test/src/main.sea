fun compose(real &a, imag &b) -> cplex:
    return a + b

fun Im(cplex &c) -> imag:
    return c as imag

fun Re(cplex &c) -> real:
    return c as real

fun main() -> int:
    real a = 1
    imag a2 = 2i
    imag b = $a2

    cplex c = compose(&a, &b)

    cprint(c)
    cprint(Im(&c))
    cprint(Re(&c))

    return 0
