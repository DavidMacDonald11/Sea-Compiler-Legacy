fun inc(var cplex &num, real re = 1, imag im = 1i):
    num += re + im

fun main() -> int:
    cplex num = 2 + 2i

    inc(&num)
    print(num as str)

    inc(&num, -2)
    print(num as str)

    inc(&num, im = -2i)
    print(num as str)

    inc(&num, im = 0i, re = 0)
    print(num as str)

    return 0
