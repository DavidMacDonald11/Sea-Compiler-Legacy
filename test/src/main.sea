fun print_array(nat[] &arr):
    nat i = len(arr)
    print("[", "")

    for nat num in arr:
        print(num as str, ", " if i > 1 else "")
        i -= 1

    print("]")

fun main(str[] args) -> int:
    nat[] arr1 = [10, 11] + [9, 8]
    print_array(&arr1)

    nat[][] arr2 = [[1, 2], [3, 4]] + [[5], [0]]

    for nat[] arr in arr2:
        print_array(&arr)

    str[] strings = ["Hello", "World!"] + ["Goodbye", "Mom"]
    strings += ["Dear"]

    for str s in strings:
        print(s)

    return 0
